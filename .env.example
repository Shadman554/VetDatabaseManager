# Veterinary API Credentials (Required)
# Get these from your veterinary API provider
VET_API_USERNAME=your_api_username
VET_API_PASSWORD=your_api_password

# Server Configuration
NODE_ENV=development
PORT=5000

# Database Configuration (Optional - for future PostgreSQL integration)
DATABASE_URL=postgresql://user:password@localhost:5432/vetdb

# Session Secret (Auto-generated in development)
SESSION_SECRET=your_secure_session_secret_here